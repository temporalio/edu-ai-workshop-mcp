{
    "image": "temporalio/gitpod-edu:1.4",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-toolsai.jupyter"
            ]
        },
        "codespaces": {
            "openFiles": [
                "README.md"
            ]
        }
    },
    "forwardPorts": [8080, 7233, 5125],
    "postStartCommand": "temporal server start-dev --ui-port 8080",
    "remoteEnv": {
        "PROMPT_DIRTRIM": "2"
    }
}